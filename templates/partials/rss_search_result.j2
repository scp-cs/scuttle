<template id="search-result-template">
<tr>
    <td id="result-name"></td>
    <td id="result-author"></td>
    <td>
        <form action="/article/assign-correction" method="post">
            {{ form.article_id(id='form-article') }}
            {{ form.corrector_id(id='form-corrector') }}
            {{ form.guid(id='form-guid') }}
            {{ form.link(id='form-link') }}
            {{ form.title(id='form-title') }}
            {{ form.submit(id='form-submit', class='small-button')}}
        </form>
    </td>
</tr>
</template>